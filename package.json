{
  "name": "hpmd",
  "version": "0.0.1",
  "description": "Simple vue js applications for Heads and Hands company",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack-dev-server --config webpack.config.dev.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NSkye/hpmd.git"
  },
  "author": "NSkye",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NSkye/hpmd/issues"
  },
  "homepage": "https://github.com/NSkye/hpmd#readme",
  "dependencies": {
    "vue": "^2.5.16",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.1",
    "jest-serializer-vue": "^2.0.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  }
}
